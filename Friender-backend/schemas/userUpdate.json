{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "interests": {
        "type": "string",
        "maxLength": 300
      },
      "hobbies": {
        "type": "string",
        "maxLength": 300
      },
      "image": {
        "type": "string"
      },
      "location": {
        "type": "integer",
        "pattern": "[0-9]{5}(-[0-9]{4})?"
      },
      "radius": {
        "type": "integer",
        "maximum": 500
      }
    },
    "additionalProperties": false,
    "required": []
}
